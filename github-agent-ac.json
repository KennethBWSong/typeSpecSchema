{
    "type": "AdaptiveCard",
    "$schema": "https://adaptivecards.io/schemas/adaptive-card.json",
    "version": "1.5",
    "body": [
        {
            "type": "Container",
            "$data": "${$root}",
            "items": [
                {
                    "type": "TextBlock",
                    "text": "id: ${if(id, id, 'N/A')}",
                    "wrap": true
                },
                {
                    "type": "TextBlock",
                    "text": "node_id: ${if(node_id, node_id, 'N/A')}",
                    "wrap": true
                },
                {
                    "type": "TextBlock",
                    "text": "name: ${if(name, name, 'N/A')}",
                    "wrap": true
                },
                {
                    "type": "TextBlock",
                    "text": "full_name: ${if(full_name, full_name, 'N/A')}",
                    "wrap": true
                },
                {
                    "type": "TextBlock",
                    "text": "license.key: ${if(license.key, license.key, 'N/A')}",
                    "wrap": true
                }
            ]
        }
    ]
}
